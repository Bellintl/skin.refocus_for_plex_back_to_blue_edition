	<!-- BIG LIST -->
	<include name="view_BigList">
		<!-- fixed focused bg -->
		<control type="group">
			<include>transition_Container</include>
			<include>infoDialogHide</include>
			<include>contextMenuHide</include>
			<control type="image">
				<posx>29</posx>
				<posy>237</posy>
				<width>1205</width>
				<height>146</height>
				<texture border="10">views/list-focus-bigAlt.png</texture>
				<visible>![Window.IsActive(movieinformation)] + [Container.Content(episodes) | Container.Content(tvshows) | Container.Content(movies)] + Control.IsVisible(51)</visible>
				<visible>!ControlGroup(300).HasFocus() + !Control.HasFocus(78)</visible>
			</control>
			<control type="image">
				<posx>29</posx>
				<posy>206</posy>
				<width>1205</width>
				<height>120</height>
				<texture border="10">views/list-focus-bigAlt.png</texture>
				<visible>Window.IsVisible(musicfiles) + Control.IsVisible(51)</visible>
				<visible>!ControlGroup(300).HasFocus() + !Control.HasFocus(78)</visible>
			</control>
		</control>
		<!-- container -->
		<control type="fixedlist" id="51">
			<viewtype label="537">biglist</viewtype>
			<posx>30</posx>
			<posy>88</posy>
			<width>1400</width>
			<height>598</height>
			<onleft>300</onleft>
			<onright>78</onright>
			<onup>51</onup>
			<ondown>51</ondown>
			<scrolltime tween="Cubic" easing="Out">200</scrolltime>
			<focusposition>1</focusposition>
			<pagecontrol>78</pagecontrol>
			<orientation>vertical</orientation>
			<include>transition2</include>
			<include>infoDialogHide</include>
			<include>contextMenuHide</include>
			<include>grainypagefade</include>
			<visible>Container.Content(movies) | Container.Content(tvshows) | Container.Content(seasons) | Container.Content(episodes) | Container.Content(albums) | Container.Content(songs)</visible>
			<include condition="Window.IsVisible(videofiles)">BigList_Video</include>
			<include condition="Window.IsVisible(musicfiles)">BigList_Music</include>
		</control>
	</include>
